#!/bin/bash
# skript, mis arvutab mitu astronoomilist tundi sa 천ppima pead oma EAP ja n채dalatega.

EAP='26'
echo -n "Palun sisesta ainepunktide arv: "
read EAPd
echo -n "Palun sisesta n채dalate arv: "
read nadal

aeg=$(( $EAP * $EAPd / $nadal ))

echo "Teile on arvestatud n채dala kohta $aeg astronoomilist tundi."
